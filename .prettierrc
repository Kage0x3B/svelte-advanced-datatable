{
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 120,
	"proseWrap": "always",
	"tabWidth": 4,
	"requireConfig": false,
	"bracketSpacing": true,
	"semi": true,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
