{
	"name": "svelte-advanced-datatable",
	"version": "0.2.6",
	"scripts": {
		"dev": "vite dev --host",
		"build": "svelte-kit sync && svelte-package && cp ./.npmrc package/",
		"build-docs": "svelte-kit sync && typedoc && node prepareApiReferenceFiles.js && vite build",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@faker-js/faker": "^7.4.0",
		"@playwright/test": "^1.25.1",
		"@sveltejs/adapter-auto": "1.0.0-next.66",
		"@sveltejs/adapter-static": "1.0.0-next.42",
		"@sveltejs/kit": "1.0.0-next.444",
		"@sveltejs/package": "1.0.0-next.1",
		"@sveltestack/svelte-query": "^1.6.0",
		"@types/github-slugger": "^1.3.0",
		"@types/luxon": "^3.0.0",
		"@types/marked": "^4.0.6",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"bootstrap": "^5.2.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"github-slugger": "^1.4.0",
		"highlight.js": "^11.6.0",
		"luxon": "^3.0.1",
		"marked": "^4.1.0",
		"mdsvex": "^0.10.6",
		"node-sass": "^7.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-highlight": "^6.2.1",
		"svelte-i18n": "^3.4.0",
		"svelte-preprocess": "^4.10.7",
		"sveltestrap": "^5.9.0",
		"tslib": "^2.4.0",
		"typedoc": "^0.23.13",
		"typedoc-plugin-markdown": "^3.13.5",
		"typedoc-plugin-mdn-links": "^2.0.0",
		"typescript": "^4.8.2",
		"unist-builder": "^3.0.0",
		"vite": "^3.0.9"
	},
	"type": "module",
	"peerDependencies": {
		"svelte": "3.x",
		"@sveltestack/svelte-query": "1.x",
		"svelte-i18n": "3.x",
		"sveltestrap": "5.x"
	},
	"peerDependenciesMeta": {
		"svelte": {
			"optional": false
		},
		"sveltestrap": {
			"optional": false
		},
		"@sveltestack/svelte-query": {
			"optional": true
		},
		"svelte-i18n": {
			"optional": true
		}
	},
	"homepage": "https://svelte-advanced-datatable.pages.dev",
	"bugs": {
		"url": "https://github.com/Kage0x3B/svelte-advanced-datatable/issues",
		"email": "moritz.hein@live.de"
	},
	"author": {
		"name": "Moritz Hein",
		"email": "moritz.hein@live.de",
		"url": "https://moritz.website"
	},
	"license": "MIT"
}
