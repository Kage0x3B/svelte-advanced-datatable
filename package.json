{
	"name": "svelte-advanced-datatable",
	"version": "0.4.3",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package && cp ./.npmrc package/",
		"build-docs": "svelte-kit sync && typedoc && node prepareApiReferenceFiles.js && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/adapter-static": "2.0.1",
		"@sveltejs/kit": "1.7.0",
		"@sveltejs/package": "1.0.2",
		"@tanstack/query-core": "^4.24.6",
		"@tanstack/svelte-query": "^4.24.6",
		"@types/luxon": "^3.2.0",
		"@types/marked": "^4.0.8",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"bootstrap": "^5.2.3",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"github-slugger": "^2.0.0",
		"highlight.js": "^11.7.0",
		"luxon": "^3.2.1",
		"marked": "^4.2.12",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-highlight": "^7.2.0",
		"svelte-i18n": "^3.6.0",
		"svelte-preprocess": "^5.0.1",
		"sveltestrap": "^5.10.0",
		"tslib": "^2.5.0",
		"typedoc": "^0.23.25",
		"typedoc-plugin-markdown": "^3.14.0",
		"typedoc-plugin-mdn-links": "^2.0.2",
		"typescript": "^4.9.5",
		"unist-builder": "^3.0.1",
		"vite": "^4.1.1",
		"vitest": "^0.28.5"
	},
	"type": "module",
	"peerDependencies": {
		"svelte": "3.x",
		"svelte-i18n": "3.x",
		"sveltestrap": "5.x"
	},
	"peerDependenciesMeta": {
		"svelte": {
			"optional": false
		},
		"sveltestrap": {
			"optional": true
		},
		"@tanstack/svelte-query": {
			"optional": true
		},
		"svelte-i18n": {
			"optional": true
		}
	},
	"homepage": "https://svelte-advanced-datatable.pages.dev",
	"bugs": {
		"url": "https://github.com/Kage0x3B/svelte-advanced-datatable/issues",
		"email": "moritz.hein@live.de"
	},
	"author": {
		"name": "Moritz Hein",
		"email": "moritz.hein@live.de",
		"url": "https://moritz.website"
	},
	"license": "MIT"
}
